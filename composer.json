{
  "name": "make-it-app/laravel-user-roles",
  "description": "A Laravel package with the role subsystem",
  "keywords": [
    "laravel",
    "user",
    "group",
    "grous",
    "role",
    "roles"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Stanislav Kabin",
      "email": "ioffline@make-it.app"
    }
  ],
  "support": {
    "issues": "https://github.com/make-it-app/laravel-user-roles/issues",
    "source": "https://github.com/make-it-app/laravel-user-roles"
  },
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "laravel/framework": "^10.10"
  },
  "require-dev": {
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "MakeIT\\UserRoles\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "MakeIT\\UserRoles\\UserRolesServiceProvider"
      ]
    },
    "aliases": {
      "Role": "MakeIT\\UserRoles\\RoleFacade"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true    
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
